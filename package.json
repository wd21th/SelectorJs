{
	"name": "selector-js",
	"displayName": "Selector Js",
	"description": "transfers Html elements to JavaScript variables",
	"version": "0.0.4",
	"engines": {
		"vscode": "^1.54.0"
	},
	"categories": [
		"Other"
	],
	"publisher": "DiasBakahbay",
	"icon": "images/icon.JPG",
	"author": {
		"name": "Dias Bakashbay"
	},
	"keywords": [
		"Selector",
		"Selector JS",
		"Snippet",
		"Html",
		"css",
		"javascript",
		"Js",
		"autocomplete",
		"auto",
		"query"
	],
	"repository": {
		"type": "git",
		"url": "https://github.com/wd21th/SelectorJs.git"
	},
	"activationEvents": [
		"onCommand:selector-js.ce",
		"onCommand:selector-js.qsby",
		"onCommand:selector-js.qsaby",
		"onCommand:selector-js.qswd",
		"onCommand:selector-js.qs",
		"onCommand:selector-js.qsa",
		"onCommand:selector-js.qswe"
	],
	"main": "./extension.js",
	"contributes": {
		"commands": [
			{
				"command": "selector-js.ce",
				"category": "Selector Js",
				"title": "Create Element (convert)"
			},
			{
				"command": "selector-js.qsby",
				"category": "Selector Js",
				"title": "Selector by ..."
			},
			{
				"command": "selector-js.qsaby",
				"category": "Selector Js",
				"title": "Selector all by ..."
			},
			{
				"command": "selector-js.qswd",
				"category": "Selector Js",
				"title": "Selector with details"
			},
			{
				"command": "selector-js.qs",
				"category": "Selector Js",
				"title": "Query selector"
			},
			{
				"command": "selector-js.qsa",
				"category": "Selector Js",
				"title": "Query selector all"
			},
			{
				"command": "selector-js.qswe",
				"category": "Selector Js",
				"title": "Selector with event"
			}
		]
	},
	"scripts": {
		"lint": "eslint .",
		"pretest": "npm run lint",
		"test": "node ./test/runTest.js"
	},
	"devDependencies": {
		"@types/vscode": "^1.54.0",
		"@types/glob": "^7.1.3",
		"@types/mocha": "^8.0.4",
		"@types/node": "^12.11.7",
		"eslint": "^7.19.0",
		"glob": "^7.1.6",
		"mocha": "^8.2.1",
		"typescript": "^4.1.3",
		"vscode-test": "^1.5.0"
	},
	"dependencies": {
		"copy-paste": "^1.3.0",
		"node-html-parser": "^3.0.4"
	}
}
